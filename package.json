{
  "name": "danmage",
  "version": "0.0.1",
  "description": "Backend server, web apps and chrome extension for DanMage.",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/teststaybaka/danmage.git"
  },
  "keywords": [
    "Danmage",
    "TypeScript"
  ],
  "author": "Yuxuan Xie <teststaybaka@gmail.com> (http://www.yuxuanxie.com)",
  "license": "GPL-3.0-or-later",
  "bugs": {
    "url": "https://github.com/teststaybaka/danmage/issues"
  },
  "homepage": "https://github.com/teststaybaka/danmage#readme",
  "devDependencies": {
    "@selfage/bundler_cli": "^5.7.0",
    "@selfage/cli": "^5.0.3",
    "@selfage/counter": "^2.0.0",
    "@selfage/element": "^4.0.3",
    "@selfage/generator_cli": "^4.4.9",
    "@selfage/puppeteer_test_executor_api": "^5.2.1",
    "@selfage/puppeteer_test_runner": "^1.3.0",
    "@selfage/screenshot_test_matcher": "^3.1.3",
    "@selfage/tabs": "^3.0.1",
    "@selfage/test_matcher": "^2.0.0",
    "@selfage/test_runner": "^5.2.0",
    "@selfage/tsconfig": "^1.1.0",
    "@selfage/web_service_client": "^3.3.4",
    "@types/chrome": "^0.0.267",
    "@types/express": "^4.17.21",
    "@types/node": "^20.12.8",
    "@types/node-fetch": "^2.6.11",
    "typescript": "^5.4.5"
  },
  "dependencies": {
    "@google-cloud/datastore": "^10.0.1",
    "@google-cloud/storage": "^7.17.2",
    "@selfage/closest_locale_finder": "^1.0.2",
    "@selfage/http_error": "^1.1.2",
    "@selfage/message": "^3.1.7",
    "@selfage/ref": "^1.0.4",
    "@selfage/service_descriptor": "^5.3.2",
    "@selfage/service_handler": "^7.5.5",
    "big-integer": "^1.6.52",
    "express": "^4.21.2",
    "express-static-gzip": "^2.2.0",
    "node-fetch": "^2.7.0"
  },
  "datastoreIndex": "./backend/datastore/index",
  "assetExts": [
    ".mp4"
  ],
  "scripts": {
    "server_prod": "bundage bns backend/main_prod main_bin -t bin",
    "web_prod": "bundage bwa -ec frontend/web/entries_prod.yaml -b frontend/web -o bin/static",
    "ext_linux": "bundage bwa -ec frontend/chrome_extension/entries_prod.yaml -d -o chrome_extension_bin",
    "ext_linux_zip": "bundage bwa -ec frontend/chrome_extension/entries_prod.yaml -d -o chrome_extension_bin; cd chrome_extension_bin/; find . \\( -iname '*.gz' -o -iname '*.html' \\) -type f -delete; zip -r ../extension.zip *; cd ../",
    "ext_firefox_linux_zip": "bundage bwa -ec frontend/chrome_extension/entries_prod.yaml -o chrome_extension_bin; cp -f frontend/chrome_extension/firefox_manifest.json chrome_extension_bin/manifest.json; cd chrome_extension_bin/; find . \\( -iname '*.gz' -o -iname '*.html' \\) -type f -delete; zip -r ../extension.zip *; cd ../"
  }
}
