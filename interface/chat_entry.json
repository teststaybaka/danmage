[{
  "enum": {
    "name": "HostApp",
    "values": [{
      "name": "YouTube",
      "value": 1
    }, {
      "name": "Crunchyroll",
      "value": 2
    }, {
      "name": "Netflix",
      "value": 3
    }]
  }
}, {
  "message": {
    "name": "ChatEntry",
    "fields": [{
      "name": "id",
      "type": "string"
    }, {
      "name": "hostApp",
      "type": "HostApp"
    }, {
      "name": "hostContentId",
      "type": "string"
    }, {
      "name": "userId",
      "type": "string"
    }, {
      "name": "userNickname",
      "type": "string"
    }, {
      "name": "content",
      "type": "string"
    }, {
      "name": "timestamp",
      "type": "number",
      "comment": "If absent, the chat entry is essentially a comment."
    }, {
      "name": "created",
      "type": "number",
      "comment": "Seconds since epoch."
    }],
    "datastore": {
      "output": "../backend/datastore/chat_entry_model",
      "key": "id",
      "queries": [{
        "name": "UserHistory",
        "filters": [{
          "fieldName": "userId",
          "operator": "="
        }],
        "orderings": [{
          "fieldName": "created",
          "descending": true
        }]
      }, {
        "name": "HostContent",
        "filters": [{
          "fieldName": "hostApp",
          "operator": "="
        }, {
          "fieldName": "hostContentId",
          "operator": "="
        }]
      }]
    }
  }
}]
