{
  "manifest_version": 2,
  "name": "DanMage",
  "description": "Bring chat overlay/bullet comments/danmaku to Twitch, YouTube and Crunchyroll!",
  "version": "3.2.0",
  "icons": { "128": "image/app_icon.png" },
  "background": {
    "scripts": ["background/index.js"]
  },
  "content_scripts": [{
    "matches": [
      "https://www.youtube.com/*",
      "https://www.twitch.tv/*"
    ],
    "js": ["content_script/index.js"],
    "run_at": "document_end"
  }, {
    "matches": [
      "https://static.crunchyroll.com/*"
    ],
    "all_frames": true,
    "js": ["content_script/index.js"],
    "run_at": "document_end"
  }],
  "permissions": [
    "identity",
    "https://www.danmage.com/"
  ],
  "key": "",
  "oauth2": {
    "client_id": "796944535248-u9f0cb23hehiir74vl4u436mtp6puqrm.apps.googleusercontent.com",
    "scopes": [
      "profile"
    ]
  }
}
